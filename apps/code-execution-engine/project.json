{
  "name": "code-execution-engine",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/code-execution-engine/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/code-execution-engine",
        "main": "apps/code-execution-engine/src/api/main.ts",
        "tsConfig": "apps/code-execution-engine/tsconfig.app.json",
        "assets": ["apps/code-execution-engine/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/code-execution-engine/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "code-execution-engine:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "code-execution-engine:build:development"
        },
        "production": {
          "buildTarget": "code-execution-engine:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/code-execution-engine/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/code-execution-engine/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
