  "scripts": {
    "start:api": "node ./dist/apps/api/main",
    "debug:api": "npx nx serve api --inspect",
    "dev:api": "npx nx serve api",
    "start:cee": "node ./dist/apps/code-execution-engine/main",
    "debug:cee": "npx nx serve code-execution-engine --inspect",
    "dev:cee": "npx nx serve code-execution-engine",
    "start:client": "node ./dist/apps/client/main",
    "debug:client": "npx nx serve client --inspect",
    "dev:client": "npx nx serve client",
    "compile-typeorm-config": "npx tsc -p apps/api/tsconfig.app.json  --outDir ./dist",
    "typeorm": "npm run compile-typeorm-config && npx typeorm -d dist/apps/api/src/configs/typeorm.config.js",
    "migration:generate": "npm run typeorm -- migration:generate --outputJs",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "generate-api-client": "npx @rtk-query/codegen-openapi libs/api-client/src/config/openapi-config.ts"
  },
